{
  "name": "videojs-mux",
  "version": "4.6.4",
  "author": "Mux, Inc.",
  "description": "Mux analytics plugin for Video.js",
  "main": "dist/videojs-mux.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "npm:audit": "npm audit --audit-level high",
    "clean": "rm -rf build dist",
    "mkdirs": "mkdir -p build dist",
    "lint": "eslint 'src/**/*.js' 'test/**/*.js' 'scripts/*.js' --quiet",
    "test": "karma start test/karma.conf.js",
    "package": "npm-run-all clean mkdirs package:build package:remove-requirejs-support package:bannerize",
    "package:build": "webpack --config webpack.prod.js",
    "package:remove-requirejs-support": "echo '(function(){var define=false;' > dist/videojs-mux.js && cat build/videojs-mux.js >> dist/videojs-mux.js && echo '})();' >> dist/videojs-mux.js",
    "package:bannerize": "bannerize dist/videojs-mux.js --banner=scripts/banner.ejs",
    "start": "npm run start:server & npm run start:test",
    "start:test": "WATCH=1 npm run test",
    "start:server": "webpack-dev-server --open --config webpack.dev.js",
    "deploy": "npm run package && babel-node scripts/deploy.js"
  },
  "keywords": [
    "videojs",
    "videojs-plugin"
  ],
  "devDependencies": {
    "aws-sdk": "^2.2.37",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.5.0",
    "bannerize": "^1.0.2",
    "chai": "^3.4.1",
    "eslint": "^2.13.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "global": "^4.3.0",
    "istanbul": "^0.4.4",
    "istanbul-instrumenter-loader": "^3.0.0",
    "json": "^9.0.3",
    "karma": "^1.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-detect-browsers": "^2.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-opera-launcher": "^1.0.0",
    "karma-safari-applescript-launcher": "^0.1.0",
    "karma-sauce-launcher": "^1.0.0",
    "karma-source-map-support": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "^2.5.3",
    "npm-run-all": "^2.2.0",
    "path": "^0.12.7",
    "sinon": "^3.0.0",
    "uglify-js": "^2.5.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "video.js": "7.2.2",
    "videojs-contrib-ads": "^5.1.0",
    "videojs-ima": "^0.7.0",
    "webpack": "^3.6.0",
    "webpack-auto-inject-version": "^1.0.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "mux-embed": "4.8.0"
  }
}